{
  "name": "rombot",
  "scripts": {
    "dev": "ts-node ./src/index.ts",
    "dev:watch": "nodemon ./src/index.ts",
    "lint": "eslint ./src",
    "lint:watch": "nodemon --watch ./src --ext ts --exec \"yarn lint\"",
    "build": "tsc",
    "start": "node ./dist/index.js",
    "migrate:create": "sh -c 'ts-node node_modules/.bin/migrate-mongo create $0 -f migrate-mongo-config.ts' ",
    "migrate:status": "ts-node node_modules/.bin/migrate-mongo status -f migrate-mongo-config.ts",
    "migrate:undo-last": "ts-node node_modules/.bin/migrate-mongo down -f migrate-mongo-config.ts && npm run migrate:status",
    "migrate": "yarn migrate:up",
    "migrate:up": "ts-node node_modules/.bin/migrate-mongo up -f migrate-mongo-config.ts",
    "migrate:down": "ts-node node_modules/.bin/migrate-mongo down -f migrate-mongo-config.ts"
  },
  "dependencies": {
    "@discordjs/builders": "^0.5.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "@types/needle": "^2.5.2",
    "@types/node": "^16.6.2",
    "@types/string-similarity": "^4.0.0",
    "@types/tough-cookie": "^4.0.1",
    "bottleneck": "^2.19.5",
    "canvas": "^2.8.0",
    "cheerio": "^1.0.0-rc.10",
    "discord-api-types": "^0.22.0",
    "discord.js": "^13.1.0",
    "duck-duck-scrape": "^2.1.2",
    "fast-average-color-node": "^1.0.3",
    "fp-ts": "^2.11.1",
    "fp-ts-std": "^0.11.0",
    "got": "^11.8.2",
    "migrate-mongo": "^8.2.3",
    "mongoose": "^5.13.7",
    "nanoid": "^3.1.25",
    "strict-event-emitter-types": "^2.0.0",
    "string-similarity": "^4.0.4",
    "tough-cookie": "^4.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-fp-ts": "^0.2.1",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-unicorn": "^35.0.0",
    "nodemon": "^2.0.12",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5"
  }
}
